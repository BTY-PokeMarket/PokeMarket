<%- include('../partials/header') %>
<body>
        <% if (poke) { %>
          <table>
            <thead>
              <tr>
                <th></th>
                <th>Pokemon</th>
                <th></th>
              </tr>
            </thead>
            <tbody>
              <% poke.forEach ((p) => { %>
                <tr>
                <td><img src="https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/<%= p.dex %>.png"></td>
                <td><%= p.name %></td>
              <td><a href="pokemon/<%= p.id %>/details">Details</a></td>
              <td></td>
              <% }) %>
              </tr>
        <% } %>
      </tbody>
      </table>
<%- include('../partials/footer') %>